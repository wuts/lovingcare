<h1>New broker</h1>

<%= error_messages_for :broker %>

<% form_for(@broker) do |f| %>
  <p>

    <%= f.hidden_field :user_id %>
  </p>

  <p>

    <%= f.hidden_field :item_id %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description,:rows=>"10" %>
  </p>



  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', brokers_path %>
